{
  "name": "@react-static/scripts",
  "version": "0.1.0",
  "description": "CLI scripts for react-static projects. This is meant to be added to the devDependencies of projects.",
  "license": "MIT",
  "author": "Derk-Jan Karrenbeld <derk-jan@xpbytes.com>",
  "repository": "https://github.com/react-static/react-static/tree/master/packages/react-static-scripts",
  "homepage": "http://react-static.js.org",
  "bin": {
    "react-static-scripts": "dist/cli.js"
  },
  "engines": {
    "node": ">= 12.13.1"
  },
  "types": "src/index.d.ts",
  "scripts": {
    "prebuild": "(rm tsconfig.tsbuildinfo -f || true)",
    "build": "yarn build:incremental --delete-dir-on-start && yarn build:declarations && yarn build:templates",
    "build:incremental": "cross-env BABEL_ENV=build yarn babel src --out-dir dist --source-maps --extensions .ts,.tsx --root-mode upward --no-comments",
    "build:declarations": "exit 0 # cli should not have types",
    "build:declarations:watch": "yarn build:declarations --watch",
    "build:templates": "cp src/templates dist -r",
    "watch": "yarn build:incremental --watch",
    "lint": "eslint .",
    "type-check": "yarn tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "test": "yarn jest",
    "test:watch": "yarn test --watch"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "peerDependencies": {
    "@react-static/core": "^0.1.0",
    "@testing-library/react": "9.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "dependencies": {
    "@react-static/eslint-config": "^0.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.4.1",
    "cross-spawn": "^7.0.1",
    "fs-extra": "^8.1.0",
    "hard-rejection": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "ink": "^2.5.0",
    "ink-box": "^1.0.0",
    "ink-confirm-input": "^2.0.0",
    "ink-select-input": "^3.1.2",
    "ink-spinner": "^3.0.1",
    "ink-text-input": "^3.2.1",
    "jest": "^24.9.0",
    "lodash.padend": "^4.6.1",
    "lodash.trimstart": "^4.5.1",
    "meow": "^5.0.0",
    "portfinder": "^1.0.25",
    "raw-loader": "^3.1.0",
    "socket.io": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0",
    "wordwrap": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@react-static/core": "^0.1.0",
    "@react-static/types": "^0.1.0",
    "@testing-library/react": "^9.3.2",
    "@types/cross-spawn": "^6.0.1",
    "@types/fs-extra": "^8.0.1",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.22",
    "@types/meow": "^5.0.0",
    "@types/socket.io": "^2.1.4",
    "@types/webpack": "^4.39.8",
    "@types/webpack-dev-server": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-jest": "^24.9.0",
    "chalk": "^3.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "ink-testing-library": "^1.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "typescript": "^3.7.2"
  }
}
