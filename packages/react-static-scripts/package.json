{
  "name": "@react-static/scripts",
  "version": "0.1.0",
  "description": "CLI scripts for react-static projects. This is meant to be added to the devDependencies.",
  "license": "MIT",
  "author": "Derk-Jan Karrenbeld <derk-jan@xpbytes.com>",
  "repository": "https://github.com/react-static/react-static/tree/master/packages/react-static-scripts",
  "homepage": "http://react-static.js.org",
  "bin": {
    "react-static-scripts": "dist/cli.js"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=build yarn babel src --out-dir dist --source-maps --extensions .ts,.tsx --delete-dir-on-start --root-mode upward --no-comments",
    "watch": "yarn build --watch",
    "lint": "eslint . && yarn lint:types",
    "lint:types": "yarn tsc",
    "build:declarations": "yarn tsc --emitDeclarationOnly",
    "test": "yarn jest",
    "test:watch": "yarn test --watch"
  },
  "files": [
    "dist/*"
  ],
  "peerDependencies": {
    "@react-static/core": "~0.1.0",
    "react": "16.x",
    "react-dom": "16.x",
    "@testing-library/react": "9.x"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "cross-spawn": "^7.0.1",
    "fs-extra": "^8.1.0",
    "hard-rejection": "^2.1.0",
    "ink": "^2.5.0",
    "ink-box": "^1.0.0",
    "ink-confirm-input": "^2.0.0",
    "ink-select-input": "^3.1.2",
    "ink-spinner": "^3.0.1",
    "ink-text-input": "^3.2.1",
    "lodash.padend": "^4.6.1",
    "lodash.trimstart": "^4.5.1",
    "meow": "^5.0.0",
    "react": "^16.10.2",
    "wordwrap": "^1.0.0",
    "@react-static/eslint-config": "~0.1.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0"
  },
  "devDependencies": {
    "@react-static/core": "~0.1.0",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/cross-spawn": "^6.0.1",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.19",
    "@types/meow": "5.0.0",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "@testing-library/react": "9.x",
    "babel-jest": "^24.9.0",
    "chalk": "^2.4.2",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "ink-testing-library": "^1.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.4"
  }
}
