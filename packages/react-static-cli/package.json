{
	"name": "@react-static/cli",
	"description": "A progressive static site generator for React",
	"version": "8.0.0",
	"license": "MIT",
	"repository": "https://github.com/react-static/react-static",
	"bin": {
		"react-static": "dist/cli.js"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"build": "cross-env BABEL_ENV=build yarn babel src --out-dir dist --source-maps --extensions .ts,.tsx --delete-dir-on-start --root-mode upward --no-comments",
		"watch": "yarn build --watch",
		"lint": "eslint . && yarn lint:types",
		"lint:types": "yarn tsc",
		"build:declarations": "yarn tsc --emitDeclarationOnly",
		"pretest:ci": "yarn build",
		"test:ci": "yarn test",
		"posttest:ci": "yarn lint",
		"test": "yarn jest",
		"test:watch": "yarn test --watch"
	},
	"files": [
		"dist/cli.js",
		"dist/ui.js"
	],
	"dependencies": {
		"core-js": "^3.3.2",
		"cross-spawn": "^7.0.1",
		"enquirer": "^2.3.2",
		"fs-extra": "^8.1.0",
		"hard-rejection": "^2.1.0",
		"ink": "^2.5.0",
		"ink-box": "^1.0.0",
		"ink-confirm-input": "^2.0.0",
		"ink-select-input": "^3.1.2",
		"ink-spinner": "^3.0.1",
		"ink-text-input": "^3.2.1",
		"lodash.padend": "^4.6.1",
		"lodash.trimstart": "^4.5.1",
		"meow": "^5.0.0",
		"pacote": "^9.5.8",
		"prop-types": "^15.7.2",
		"react": "^16.10.2",
		"semver": "^6.3.0",
		"tar": "^5.0.5",
		"wordwrap": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"@babel/preset-typescript": "^7.6.0",
		"@types/cross-spawn": "^6.0.1",
		"@types/fs-extra": "^8.0.0",
		"@types/jest": "^24.0.19",
		"@types/meow": "5.0.0",
		"@types/pacote": "^9.5.0",
		"@types/semver": "^6.0.2",
		"@types/tar": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^2.4.0",
		"@typescript-eslint/parser": "^2.4.0",
		"babel-jest": "^24.9.0",
		"chalk": "^2.4.2",
		"cross-env": "^6.0.3",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-react-hooks": "^2.1.2",
		"ink-testing-library": "^1.0.2",
		"jest": "^24.9.0",
		"prettier": "^1.18.2",
		"typescript": "^3.6.4"
	}
}
