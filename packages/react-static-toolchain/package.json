{
  "name": "@react-static/toolchain",
  "description": "Toolchain to build @react-static packages",
  "version": "0.1.0",
  "license": "MIT",
  "repository": "https://github.com/react-static/react-static",
  "engines": {
    "node": ">= 10.16.3"
  },
  "bin": {
    "babel": "./node_modules/.bin/babel",
    "eslint": "./node_modules/.bin/eslint",
    "jest": "./node_modules/.bin/jest",
    "prettier": "./node_modules/.bin/prettier",
    "tsc": "./node_modules/.bin/tsc"
  },
  "scripts": {
    "build": "yarn babel src --out-dir dist --extensions \".ts,.tsx\"",
    "watch": "yarn build --watch",
    "lint": "eslint .",
    "lint:types": "yarn tsc",
    "build:declarations": "yarn tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.9.0",
    "core-js": "^3.2.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.2"
  }
}
